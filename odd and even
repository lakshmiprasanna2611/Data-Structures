#include<stdio.h>
int main()
{
	int i,size;
	printf("enter the size of the array:");
	scanf("%d",&size);
	int a[size];
	printf("enter the elements of the array:");
	for(i=0;i<size;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Even Numbers:");
	for(i=0;i<size;i++)
	{
		if (a[i]>=0)
		{
			if (a[i]%2==0)  printf("%d ",a[i]);
		}
	}
	printf("odd Numbers:");
	for(i=0;i<size;i++)
	{
		if (a[i]>=0)
		{
			if (a[i]%2==1)   printf("%d ",a[i]);
		}
	}
		return 0;
}


